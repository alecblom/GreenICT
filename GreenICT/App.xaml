<Application x:Class="GreenICT.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GreenICT"
             StartupUri="GameWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="BackGroundColour" Color="#5F2871" />
        <SolidColorBrush x:Key="primaryDark" Color="#32083F" />
        <SolidColorBrush x:Key="secondaryDark" Color="#0A000E" />
        <SolidColorBrush x:Key="primaryLight" Color="#925FA2" />
        <SolidColorBrush x:Key="secondaryLight" Color="#E0BFEA" />
    </Application.Resources>
</Application>
